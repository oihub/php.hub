{
    "name": "oihub/php.hub",
    "type": "library",
    "description": "Hub",
    "license": "MIT",
    "authors": [
        {
            "name": "sean",
            "email": "maoxfjob@163.com"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "ext-fileinfo": "*",
        "ext-openssl": "*",
        "ext-simplexml": "*",
        "guzzlehttp/guzzle": "^6.5.5",
        "monolog/monolog": "^2.1",
        "pimple/pimple": "^3.3",
        "psr/simple-cache": "^1.0",
        "symfony/cache": "^5.1",
        "symfony/http-foundation": "^5.1",
        "wechatpay/wechatpay-guzzle-middleware": "^0.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4"
    },
    "autoload": {
        "psr-4": {
            "PhpHos\\Hub\\": "src"
        },
        "files": [
            "src/Functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "PhpHos\\Hub\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer"
        }
    }
}
